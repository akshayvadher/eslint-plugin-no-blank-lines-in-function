{
  "name": "eslint-plugin-no-blank-lines-in-function",
  "version": "1.0.4",
  "description": "ESLint plugin to prevent blank lines inside functions",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.cjs"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build:cjs": "esbuild src/index.js --bundle --platform=node --format=cjs --outfile=dist/cjs/index.cjs",
    "build:esm": "esbuild src/index.js --bundle --platform=node --format=esm --outfile=dist/esm/index.js",
    "build": "npm run build:cjs && npm run build:esm",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "eslint",
    "plugin",
    "function",
    "blank-lines"
  ],
  "author": "Akshay Vadher",
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.0.0",
    "prettier": "^3.6.2",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akshayvadher/eslint-plugin-no-blank-lines-in-function.git"
  },
  "homepage": "https://github.com/akshayvadher/eslint-plugin-no-blank-lines-in-function#readme",
  "bugs": {
    "url": "https://github.com/akshayvadher/eslint-plugin-no-blank-lines-in-function/issues"
  }
}
